{
  "name": "nbx",
  "description": "My own personal cli",
  "version": "1.0.0",
  "author": "Nicolas Beaussart @beaussan",
  "bin": {
    "nbx": "./bin/run"
  },
  "bugs": "https://github.com/beaussart/nbx/issues",
  "dependencies": {
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/errors": "1.2.2",
    "@oclif/plugin-help": "2.2.3",
    "@oclif/plugin-not-found": "1.2.3",
    "@oclif/plugin-warn-if-update-available": "1.7.0",
    "axios": "0.19.1",
    "colors": "1.3.3",
    "cosmiconfig": "6.0.0",
    "gluegun": "4.1.2",
    "isomorphic-git": "0.78.3",
    "latest-version": "5.1.0",
    "prompts": "2.3.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@types/node": "10.17.14",
    "@types/nodegit": "0.26.1",
    "@types/prompts": "2.0.3",
    "eslint": "5.13.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-oclif-typescript": "0.1.0",
    "globby": "10.0.2",
    "husky": "4.2.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/beaussart/nbx",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nbx",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/errors",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-not-found"
    ],
    "topics": {
      "add": {
        "description": "install library to projects"
      }
    }
  },
  "repository": "beaussart/nbx",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "format:write": "prettier --write \"**/*.{js,vue,json,ts,tsx,md,yml,html}\"",
    "format:check": "prettier --list-different \"**/*.{js,vue,json,ts,tsx,md,yml,html}\""
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
